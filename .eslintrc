{
	"parser": "babel-eslint",
	"plugins": [
		"decorator",
		"react"
	],
	"env": {
		"browser": true,
		"node": true
	},
	"ecmaFeatures": {
		"jsx": true
	},
	"rules": {
		"camelcase": 2,
		"eol-last": 2,
		"eqeqeq": 2,
		"max-depth": 2,
		"no-mixed-spaces-and-tabs": 2,
		"no-var": 2,
		"quotes": [2, "single"],
		"strict": [2, "never"],
		"react/jsx-uses-vars": 1,
		"react/self-closing-comp": 1,
		"react/wrap-multilines": 1
	}
}
